# Example .env file for Assortment Planner Agent
LLM_MODEL=gpt-4o-mini
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_HOST=https://cloud.langfuse.com
INVENTORY_AGENT_CARD_URL=http://inventory-agent:8001/.well-known/agent-card
PROCUREMENT_AGENT_CARD_URL=http://procurement-agent:8002/.well-known/agent-card
PO_AGENT_CARD_URL=http://po-agent:8003/.well-known/agent-card
PUBLIC_BASE_URL=http://localhost:8000
ASSORTMENT_PORT=8000
APP_NAME=assortment-planner

# LLM provider API keys
OPENAI_API_KEY=your_openai_api_key
ANTHROPIC_API_KEY=your_anthropic_api_key
GOOGLE_API_KEY=your_google_api_key
MISTRAL_API_KEY=your_mistral_api_key
